<%- include header.ejs %>
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Voters</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                           <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                              <thead>
                                      <tr>
                                          <th>No</th>
                                          <th hidden>Voter ID</th>
                                          <th>Election Id</th>
                                          <th hidden>Address</th>
                                          <th>Name</th>
                                          <th>Gander</th>
                                          <th>vote status</th>
                                        <!--  <th>TranscationHash</th>-->
                                      </tr>
                                  </thead>
                                  <tbody>
                                    <% if(data.length){
                                        %>  <%
                                        var count=0;
                                        for(var i = 0;i < data.length;i++) { %>
                                          <tr class="odd gradeX">
                                          <td><%=count += 1%></td>
                                           <td hidden><%=data[i].voterID%></td>
                                           <td><%=data[i].voterElectionId%></td>
                                           <td hidden><%=data[i].voterAddress%></td>
                                           <td><%= (data[i].voterFastName) +' ' + (data[i].voterLastName) %></td>
                                           <% if(data[i].voterGender==1){ %>
                                            <td>Male</td>
                                        <%  }else if(data[i].voterGender==2){ %>
                                            <td>Famale</td>
                                        <%  } else { %>
                                            <td>Unique</td>
                                        <%}%>
                                           <td><%=data[i].voterVoted%></td>
                                          <!--  <td><%=data[i].TranscationHash%></td>-->
                                           </tr>
                                           <% } %>

                                        <%  }else{ %>
                                            <tr>
                                              <td>NA</td>
                                              <td hidden>NA</td>
                                              <td hidden>NA</td>
                                              <td>NA</td>
                                              <td>NA</td>
                                              <td>NA</td>

                                            </tr>
                                    <% } %>
                                  </tbody>
                              </table>

                          </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
        </div>
        <!-- /#page-wrapper -->

  <%- include footer.ejs %>
